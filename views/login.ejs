<script>
	function getQueryParams() {
		let params = {};
		let queryString = window.location.search.substring(1);
		let queryArray = queryString.split("&");

		queryArray.forEach(function (param) {
			let pair = param.split("=");
			params[pair[0]] = pair[1];
		});

		return params;
	}
</script>

